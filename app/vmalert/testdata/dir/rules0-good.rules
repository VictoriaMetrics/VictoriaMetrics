groups:
  - name: duplicatedGroupDiffFiles
    interval: 1s
    rules:
      - alert: VMRows
        for: 5m
        expr: vm_rows > 0
        labels:
          label: bar
          expr: "{{ $expr|queryEscape }}"
        annotations:
          summary: "{{ $value|humanize }}"
          description: "{{$labels}}"


