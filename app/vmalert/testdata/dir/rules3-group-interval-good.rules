groups:
  - name: groupWithInterval
    interval: 5m
    rules:
      - alert: VMRows
        for: 5m
        expr: vm_rows > 0
        labels:
          label: bar
        annotations:
          summary: "{{ $value|humanize }}"
          description: "{{$labels}}"

  - name: groupWithoutInterval
    rules:
      - alert: VMRows
        for: 5m
        expr: vm_rows > 0
        labels:
          label: bar
        annotations:
          summary: "{{ $value|humanize }}"
          description: "{{$labels}}"


