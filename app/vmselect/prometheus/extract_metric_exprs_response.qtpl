{% stripspace %}

ExtractMetricExprsResponse generates response for /extract-metric-exprs .
{% func ExtractMetricExprsResponse(metrics []string) %}
{
	"status":"success",
	"data":[
		{% if len(metrics) > 0 %}
			{%q= metrics[0] %}
			{% for i := 1; i < len(metrics); i++ %}
				,{%q= metrics[i] %}
			{% endfor %}
		{% endif %}
	]
}
{% endfunc %}

{% endstripspace %} 