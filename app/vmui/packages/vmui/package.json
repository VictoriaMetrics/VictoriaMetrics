{
  "name": "vmui",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "type": "module",
  "scripts": {
    "prestart": "npm run copy-metricsql-docs",
    "start": "vite",
    "start:playground": "cross-env PLAYGROUND=METRICS npm run start",
    "start:logs": "vite --mode victorialogs",
    "start:logs:playground": "cross-env PLAYGROUND=LOGS npm run start:logs",
    "start:anomaly": "vite --mode vmanomaly",
    "build": "vite build",
    "build:logs": "vite build --mode victorialogs",
    "build:anomaly": "vite build --mode vmanomaly",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "copy-metricsql-docs": "cp ../../../../docs/MetricsQL.md src/assets/MetricsQL.md || true",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "marked": "^15.0.12",
    "marked-emoji": "^2.0.0",
    "qs": "^6.14.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-imask": "^7.6.1",
    "react-router-dom": "^7.6.2",
    "uplot": "^1.6.32",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.get": "^4.4.9",
    "@types/node": "^24.0.3",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "^4.5.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.2.0",
    "http-proxy-middleware": "^3.0.5",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "sass": "^1.89.2",
    "sass-embedded": "^1.89.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
