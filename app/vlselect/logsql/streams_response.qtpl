{% stripspace %}

// StreamsResponse formats /select/logsql/streams response
{% func StreamsResponse(streams []string) %}
{
	"streams":[
		{% if len(streams) > 0 %}
			{%q= streams[0] %}
			{% for _, v := range streams[1:] %}
				,{%q= v %}
			{% endfor %}
		{% endif %}
	]
}
{% endfunc %}

{% endstripspace %}
