{% stripspace %}

// FieldNamesResponse formats /select/logsql/field_names response
{% func FieldNamesResponse(names []string) %}
{
	"names":[
		{% if len(names) > 0 %}
			{%q= names[0] %}
			{% for _, v := range names[1:] %}
				,{%q= v %}
			{% endfor %}
		{% endif %}
	]
}
{% endfunc %}

{% endstripspace %}
