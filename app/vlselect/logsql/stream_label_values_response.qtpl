{% stripspace %}

// StreamLabelValuesResponse formats /select/logsql/stream_label_values response
{% func StreamLabelValuesResponse(values []string) %}
{
	"values":[
		{% if len(values) > 0 %}
			{%q= values[0] %}
			{% for _, v := range values[1:] %}
				,{%q= v %}
			{% endfor %}
		{% endif %}
	]
}
{% endfunc %}

{% endstripspace %}
