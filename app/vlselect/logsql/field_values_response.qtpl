{% stripspace %}

// FieldValuesResponse formats /select/logsql/field_values response
{% func FieldValuesResponse(values []string) %}
{
	"values":[
		{% if len(values) > 0 %}
			{%q= values[0] %}
			{% for _, v := range values[1:] %}
				,{%q= v %}
			{% endfor %}
		{% endif %}
	]
}
{% endfunc %}

{% endstripspace %}
